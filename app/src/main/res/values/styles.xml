    <!--自适应-->
    <style name="wrap_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!-- 水平填充 -->
    <style name="horizontal_weight1">
        <!-- 水平填充 垂直适应.. -->
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!-- 水平填充 -->
    <style name="horizontal_weight_fill">
        <!-- 水平填充 垂直适应.. -->
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <!-- 垂直填充 -->
    <style name="vertical_weight1">
        <!-- 水平填充 垂直适应.. -->
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dip</item>
    </style>

    <!-- 水平填充 -->
    <!-- 水平填充 垂直适应.. -->
    <style name="all_fill">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>


    <style name="horizontal_line">
        <!-- 水平填充 垂直1px.. -->
        <item name="android:background">?attr/line_color</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
    </style>

    <!-- 水平填充 垂直1px.. -->
    <!-- 水平填充 垂直1px.. -->
    <style name="vertical_fill">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>


    <style name="vertical_line">
        <!-- 水平填充 垂直1px.. -->
        <item name="android:background">?attr/line_color</item>
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">match_parent</item>
    </style>

