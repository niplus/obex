<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <import type="android.graphics.drawable.Drawable" />

        <variable
            name="view"
            type="android.view.View.OnClickListener" />
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="?attr/bg_color"
        android:orientation="vertical">

        <include layout="@layout/title_include" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingLeft="12dp"
            android:paddingRight="12dp">

            <RelativeLayout
                android:id="@+id/rl_nick_name"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:clickable="false"
                    android:text="@string/safesetting_nickname"
                    android:textColor="?attr/font_color"
                    android:textSize="15sp" />

                <TextView
                    android:id="@+id/tv_nick_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:clickable="false"
                    android:drawablePadding="10dp"
                    android:drawableRight="?attr/icon_right"
                    android:text="@string/safesetting_unsetting"
                    android:textColor="?attr/font_color3"
                    android:textSize="13sp" />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_password"
                android:layout_width="match_parent"
                android:layout_height="50dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:clickable="false"
                    android:text="@string/safesetting_login"
                    android:textColor="?attr/font_color"
                    android:textSize="15sp" />

                <TextView
                    android:id="@+id/tv_login"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:clickable="false"
                    android:drawablePadding="10dp"
                    android:drawableRight="?attr/icon_right"
                    android:text="@string/safesetting_password_reset"
                    android:textColor="?attr/font_color3"
                    android:textSize="13sp" />


            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/rl_bindphone"
                android:layout_width="match_parent"
                android:layout_height="50dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:clickable="false"
                    android:text="@string/safesetting_safe"
                    android:textColor="?attr/font_color"
                    android:textSize="15sp" />

                <TextView
                    android:id="@+id/tv_bindphone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:clickable="false"
                    android:drawablePadding="10dp"
                    android:drawableRight="?attr/icon_right"
                    android:text="@string/safesetting_unsetting"
                    android:textColor="?attr/font_color3"
                    android:textSize="13sp" />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_bindemail"
                android:layout_width="match_parent"
                android:layout_height="50dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:clickable="false"
                    android:text="@string/safesetting_email"
                    android:textColor="?attr/font_color"
                    android:textSize="15sp" />

                <TextView
                    android:id="@+id/tv_email"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:clickable="false"
                    android:drawablePadding="10dp"
                    android:drawableRight="?attr/icon_right"
                    android:text="@string/safesetting_unsetting"
                    android:textColor="?attr/font_color3"
                    android:textSize="13sp" />


            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/rl_bindgoogle"
                android:layout_width="match_parent"
                android:layout_height="50dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:clickable="false"
                    android:text="@string/safesetting_google"
                    android:textColor="?attr/font_color"
                    android:textSize="15sp" />

                <TextView
                    android:id="@+id/tv_google"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:clickable="false"
                    android:drawablePadding="10dp"
                    android:drawableRight="?attr/icon_right"
                    android:text="@string/safesetting_unsetting"
                    android:textColor="?attr/font_color3"
                    android:textSize="13sp" />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_zijin"
                android:layout_width="match_parent"
                android:layout_height="50dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:clickable="false"
                    android:text="@string/safesetting_zijin"
                    android:textColor="?attr/font_color"
                    android:textSize="15sp" />

                <TextView
                    android:id="@+id/tv_zijin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:clickable="false"
                    android:drawablePadding="10dp"
                    android:drawableRight="?attr/icon_right"
                    android:text="@string/safesetting_unsetting"
                    android:textColor="?attr/font_color3"
                    android:textSize="13sp" />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_shoushi"
                android:layout_width="match_parent"
                android:layout_height="50dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:clickable="false"
                    android:text="@string/safesetting_shoushi"
                    android:textColor="?attr/font_color"
                    android:textSize="15sp" />

                <TextView
                    android:id="@+id/tv_shoushi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:clickable="false"
                    android:drawablePadding="10dp"
                    android:drawableRight="?attr/icon_right"
                    android:text="@string/safesetting_unsetting"
                    android:textColor="?attr/font_color3"
                    android:textSize="13sp" />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_zhiwen"
                android:layout_width="match_parent"
                android:layout_height="50dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:clickable="false"
                    android:text="@string/safesetting_zhiwen"
                    android:textColor="?attr/font_color"
                    android:textSize="15sp" />

                <TextView
                    android:id="@+id/tv_zhiwen"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:clickable="false"
                    android:drawablePadding="10dp"
                    android:drawableRight="?attr/icon_right"
                    android:text="@string/safesetting_unsetting"
                    android:textColor="?attr/font_color3"
                    android:textSize="13sp" />


            </RelativeLayout>
        </LinearLayout>

    </LinearLayout>


</layout>